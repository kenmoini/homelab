local-address=0.0.0.0
local-port={{ pdns_recursor_port }}
packetcache-servfail-ttl=1
packetcache-negative-ttl=1
max-cache-ttl=15
packetcache-ttl=15
max-cache-bogus-ttl=1
max-negative-ttl=1
nothing-below-nxdomain=no
trace=on
allow-from=127.0.0.0/8, 10.0.0.0/8, 100.64.0.0/10, 169.254.0.0/16, 192.168.0.0/16, 172.16.0.0/12, ::1/128, fc00::/7, fe80::/10
security-poll-suffix=
setgid=pdns-recursor
setuid=pdns-recursor
dnssec=off

{% if pdns_upstream_servers is defined %}
forward-zones-recurse={% for server in pdns_upstream_servers %}.={{ server }}{% if not loop.last %},{% endif %}{% endfor %}
{% endif %}

{% if pdns_forward_zones is defined %}
{% for zone in pdns_forward_zones %}
forward-zones{% if not loop.first %}+{% endif %}={{ zone.name }}.={{ zone.server }}
{% endfor %}
{% endif %}

