CORS_ORIGIN_ALLOW_ALL=True
DB_HOST={{ netbox_db_host }}
DB_NAME={{ netbox_db_name }}
DB_PASSWORD={{ netbox_db_password }}
DB_USER={{ netbox_db_username }}
DB_WAIT_DEBUG=1
EMAIL_FROM={{ netbox_email_from }}
EMAIL_PASSWORD=
EMAIL_PORT=25
EMAIL_SERVER=localhost
EMAIL_SSL_CERTFILE=
EMAIL_SSL_KEYFILE=
EMAIL_TIMEOUT=5
EMAIL_USERNAME=netbox
# EMAIL_USE_SSL and EMAIL_USE_TLS are mutually exclusive, i.e. they can't both be `true`!
EMAIL_USE_SSL=false
EMAIL_USE_TLS=false
GRAPHQL_ENABLED=true
HOUSEKEEPING_INTERVAL=86400
MEDIA_ROOT=/opt/netbox/netbox/media
METRICS_ENABLED=true
prometheus_multiproc_dir: /tmp/metrics
REDIS_CACHE_DATABASE=1
REDIS_CACHE_HOST=localhost
REDIS_CACHE_INSECURE_SKIP_TLS_VERIFY=false
REDIS_CACHE_PASSWORD={{ netbox_cache_password }}
REDIS_CACHE_SSL=false
REDIS_CACHE_PORT={{ netbox_cache_port }}
REDIS_DATABASE=0
REDIS_HOST=localhost
REDIS_INSECURE_SKIP_TLS_VERIFY=false
REDIS_PASSWORD={{ netbox_redis_password }}
REDIS_SSL=false
REDIS_PORT={{ netbox_redis_port }}
RELEASE_CHECK_URL=https://api.github.com/repos/netbox-community/netbox/releases
SKIP_SUPERUSER={{ netbox_skip_superuser }}
WEBHOOKS_ENABLED=true